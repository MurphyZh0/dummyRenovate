{
    "gitAuthor": "Renovate Bot <ServiceAccount.renovate@citrix.com>",
    "requireConfig": false,
    "onboarding": false,
    "extends": [
        "config:base",
        ":preserveSemverRanges",
        ":rebaseStalePrs",
        ":semanticCommits",
        "github>whitesource/merge-confidence:beta",
        "github>whitesource/log4j-remediations",
        "github>renovatebot/spring-remediations"
    ],
    "branchConcurrentLimit": 5,
    "prConcurrentLimit": 5,
    "lockFileMaintenance": { "enabled": true },
    "packageRules": [
        {
          "matchDepTypes": ["devDependencies"],
          "prPriority": -1
        }
    ],
    "stabilityDays": 3
}
